{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "tsc": "tsc",
    "clean": "rimraf dest/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dest",
    "build": "npm run clean && tsc && npm run copy-files",
    "dev": "ts-node-dev --respawn --transpile-only ./src/bin/www",
    "prod": "npm run build && node ./src/bin/www"
  },
  "dependencies": {
    "arangojs": "^7.5.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0",
    "rxjs": "^7.1.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/morgan": "^1.9.2"
  }
}
